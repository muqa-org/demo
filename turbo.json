{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": [
        "NEXT_PUBLIC_COMETH_API_KEY",
        "PINATA_JWT",
        "PINATA_GATEWAY_URL"
      ],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "nuke": {
      "outputs": []
    },
    "construct": {
      "outputs": []
    },
    "reconstruct": {
      "outputs": []
    },
    "start": {
      "cache": false
    },
    "db:generate": {
      "outputs": []
    }
  }
}
